<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="studentActionForm"
			type="com.sudha.StudentActionForm" />
	</form-beans>
	<action-mappings>
		<action path="/reg" name="studentActionForm"
			type="com.sudha.StudentAction">
			<forward name="success" path="/success.html" />
			<forward name="failure" path="/failure.html" />
			<forward name="existed" path="/existed.html" />
		</action>
	</action-mappings>
</struts-config>       